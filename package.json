{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "obsidian-extensions",
  "title": "Obsidian extensions",
  "description": "",
  "icon": "command-icon.png",
  "author": "ejohane",
  "license": "MIT",
  "commands": [
    {
      "name": "index",
      "title": "Add Daily Note",
      "description": "Add a note to your daily note in Obsidian.",
      "mode": "view"
    },
    {
      "name": "todo",
      "title": "To Dos",
      "mode": "view",
      "description": "Interact with list of To Dos from Obsidian"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.43.2",
    "@raycast/utils": "^1.4.10",
    "date-fns": "^2.29.3"
  },
  "preferences": [
    {
      "name": "vault",
      "description": "vault location",
      "type": "directory",
      "required": true,
      "title": "vault"
    }
  ],
  "devDependencies": {
    "@types/jest": "^29.2.2",
    "@types/node": "18.8.3",
    "@types/react": "18.0.9",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "jest": "^29.3.1",
    "prettier": "^2.5.1",
    "ts-jest": "^29.0.3",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "ray publish",
    "test": "jest ./src/*"
  }
}
